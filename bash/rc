if [ -e ~/.cache/wal/sequences ]; then (cat ~/.cache/wal/sequences &); fi
if [ -e "$HOME"/.local/share/bash-completion/completions/deno.bash ]; then . "$HOME"/.local/share/bash-completion/completions/deno.bash; fi
if [ -e /usr/share/nvm/init-nvm.sh ]; then . /usr/share/nvm/init-nvm.sh; fi
set -o vi

alias ls='ls --color=auto -F'
alias youtube-dl-album='yt-dlp --config-location $HOME/.config/youtube-dl/album'
alias youtube-dl-track='yt-dlp --config-location $HOME/.config/youtube-dl/track'
alias v=vim
alias h=helix
